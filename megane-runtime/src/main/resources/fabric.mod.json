{
  "schemaVersion": 1,
  "id"           : "megane-runtime",
  "version"      : "${version}",
  "description"  : "for things to work",
  "authors"      : [
    "deirn"
  ],
  "contributors" : [
    "qsefthuopq - Simplified Chinese Translator"
  ],
  "contact"      : {
    "homepage": "https://github.com/badasintended/megane",
